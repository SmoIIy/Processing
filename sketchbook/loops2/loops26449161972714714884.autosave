

void setup() {
  size(600, 600);
  background(0);
}

void draw() {
  strokeWeight(1);
  stroke(255);
  
  fill(255);
  

  for (int y = 0; y < height; y += 75) {
    for (int x = 0; x < width; x += 75) {
      rect(x, y, 75, 75);
    }
  }
  
  for (int y = 0; y < height; y += 150) {
    for (int x = 0; x < width; x += 150) {
      fill(0);
      rect(x, y, 75, 75);
    }
  }
}
